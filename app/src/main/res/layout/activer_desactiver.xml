        android:textSize="50dp"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="326dp"
        app:layout_constraintCircleRadius="25dp"

        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="244dp"
